<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/menuvi"

    tools:context=".Visiteurmenu">

    <Button
        android:id="@+id/demande"
        android:layout_width="105dp"
        android:layout_height="36dp"
        android:layout_marginTop="252dp"
        android:backgroundTint="#F6DD81"
        android:text="Demande"
        android:textColor="#CA492D"


        android:textSize="11sp"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/contact"
        android:layout_width="109dp"
        android:layout_height="37dp"
        android:layout_marginTop="504dp"
        android:backgroundTint="#F6DD81"
        android:text="Contact"
        android:textColor="#CA492D"

        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Internat"
        android:layout_width="112dp"
        android:layout_height="38dp"
        android:layout_marginTop="252dp"
        android:backgroundTint="#F6DD81"
        android:text="Internat"
        android:textColor="#CA492D"


        android:textSize="11sp"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/faq"
        android:layout_width="109dp"
        android:layout_height="37dp"
        android:layout_marginTop="504dp"

        android:backgroundTint="#F6DD81"
        android:text="FAQ"
        android:textColor="#CA492D"

        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.691"
        app:layout_constraintStart_toEndOf="@+id/contact"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>